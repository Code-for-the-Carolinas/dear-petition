{
  "addons": [
    "mailgun",
    "heroku-postgresql",
    "heroku-redis"
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "heroku/python"
    },
    {
      "url": "https://github.com/carwow/heroku-buildpack-pdftotext.git"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": {
      "value": "config.settings.production"
    },
    "DJANGO_SECRET_KEY": {
      "description": "Django's secret key",
      "generator": "secret"
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "name": "dear-petition",
  "scripts": {
  },
  "stack": "heroku-18"
}
